    "import pandas as pd\n",
    "import plotly.express as px\n",
    "\n",
    "# Leer los datos\n",
    "car_data = pd.read_csv('vehicles_us.csv')\n",
    "\n",
    "# Crear un histograma\n",
    "fig = px.histogram(car_data, x=\"odometer\")\n",
    "fig.show()\n",
    "\n",
    "# Crear un gráfico de dispersión\n",
    "fig = px.scatter(car_data, x=\"odometer\", y=\"price\")\n",
    "fig.show()"
   ]
